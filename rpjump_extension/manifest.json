{
  "manifest_version": 3,
  "name": "RP Jump",
  "version": "1.5",
  "description": "Jump from ReportPortal test failures directly to the Magna logs.",
  "permissions": ["activeTab", "scripting", "contextMenus", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": "icon_alert.png",
    "default_title": "RP Jump"
  },
  "options_page": "options.html"
}
